{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "short_name": "Movie App",
  "name": "Movie App",
  "description": "Free Download movies easy with GetMovie app .\n Download 720p,1080p,3D movies from YIFI api ",
  "scope": "/",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "48x48",
      "type": "image/x-icon"
    },
    {
      "src": "maskable_icon.png",
      "sizes": "192x192",
      "type": "image/x-icon",
      "purpose": "any maskable"
    },
    {
      "src": "/android-chrome-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "/android-chrome-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#2d2d2d",
  "shortcuts": [
    {
      "name": "Browse movies",
      "short_name": "Home",
      "description": "Browse Movies as you like",
      "url": "/",
      "icons": [{ "src": "/icons/movie_icon.png", "sizes": "192x192" }]
    },
    {
      "name": "Watchlist",
      "short_name": "Bookmark",
      "description": "Movies you saved for later",
      "url": "/watchlist",
      "icons": [{ "src": "/icons/watchlist_icon.png", "sizes": "192x192" }]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshot/screenshot_00.webp",
      "sizes": "1280x720",
      "type": "image/webp"
    }
  ]
}
